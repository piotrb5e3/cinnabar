x = #{
  1: 1,
  2: 2,
  3: "Hello",
};

x["42"] = " World";
assert x["4" + ['2']] == " World";

x[1] = lambda: 1;

assert x[1]() == 1;

assert x.keys == [1, 2, 3, "42"];

assert x.keys_values == [[1, x[1]], [2, 2], [3, "Hello"], ["42", " World"]];

assert x.keys_values.length == 4;

z = lambda xx: xx^2;
assert #{ 1 + 2: 3 + 1, "a" + "b": z} == #{4 - 1: 80 - 76, "ab": z};

print "OK\n";
