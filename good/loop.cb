x = 99;
while (x > 0) {
  print str(x) + " bottles of beer on the wall, " + str(x) + " bottles of beer.\n"
    + "Take one down, pass it around, " + str(x - 1) + " bottles of beer on the wall.\n";
  x = x - 1;
}

print "No more bottles of beer on the wall, no more bottles of beer.\n"
  + "We've taken them down and passed them around; now we're drunk and passed out!\n";
