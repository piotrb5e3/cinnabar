x = 99;
while (x > 0) {
  print x; print " bottles of beer on the wall, "; print x; print " bottles of beer.\n";
  print "Take one down, pass it around, "; print x - 1; print " bottles of beer on the wall.\n";
  x = x - 1;
}

print "No more bottles of beer on the wall, no more bottles of beer.\n"
  + "We've taken them down and passed them around; now we're drunk and passed out!\n";
