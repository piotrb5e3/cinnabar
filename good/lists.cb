l = [1, 2, 3, 4, 5];
{l0, l1, l2} = l;
assert l0 == 1 && l1 == 2 && l2 == 3;

l[0] = 123;
{l0, l1, l2} = l;
assert l0 == 123;

l[0] = l[0] + l[1] + l[l[1]];
assert l[0] == 128;
ll = [i ^ i ^ i for i in l];
assert ll[1] == 16;

z = l + ll;
assert ll.length == 10;

assert [1, 2, 3] * 3 == [1, 2, 3, 1, 2, 3, 1, 2, 3];
