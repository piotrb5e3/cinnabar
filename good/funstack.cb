push = lambda stack, value: lambda: [stack, value];

pop = lambda stack: stack();

s = 0;
s = push(s, 11);
s = push(s, 99);
s = push(s, 21);
{s, v} = pop(s);
assert v == 21;
s = push(s, 88);
s = push(s, 673);
{s, v} = pop(s);
assert v == 673;
{s, v} = pop(s);
assert v == 88;
{s, v} = pop(s);
assert v == 99;
s = push(s, 11);

