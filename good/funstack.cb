push = lambda stack, value: [stack, value];

pop = lambda stack: stack();

s = 0;
s = push(s, 11);
s = push(s, 99);
s = push(s, 21);
{s, v} = pop(s);  //v == 21
s = push(s, 88);
s = push(s, 673);
{s, v} = pop(s);  // v == 673
{s, v} = pop(s);  // v == 88
{s, v} = pop(s);  // v == 99
s = push(s, 11);

